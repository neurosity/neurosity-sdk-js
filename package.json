{
  "name": "@neurosity/sdk",
  "version": "7.0.0",
  "description": "Neurosity SDK",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "browser": "dist/neurosity.umd.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "browser": "./dist/neurosity.umd.js"
    }
  },
  "scripts": {
    "prebuild": "npm run clean",
    "build": "npm run lint && rollup -c && npm run examples:browser:build",
    "build:docs": "npm run build:typedoc && cd website && npm install && npm run build",
    "build:typedoc": "rm -rf docs/reference && typedoc",
    "examples:browser:build": "rm -rf ./dist/examples && copyfiles -a ./examples/**/* ./dist",
    "examples:browser:start": "lite-server --baseDir=./dist/examples/browser",
    "examples:node:start": "npx tsx examples/node",
    "dev": "rollup -c -w",
    "start": "npm run dev",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run build",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/neurosity/neurosity-sdk-js.git"
  },
  "keywords": [
    "neurosity",
    "api",
    "client",
    "neurosity"
  ],
  "engines": {
    "node": ">=20"
  },
  "author": "Alex Castillo",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/neurosity/neurosity-sdk-js/issues"
  },
  "homepage": "https://docs.neurosity.co",
  "dependencies": {
    "@neurosity/ipk": "^2.12.0",
    "axios": "^1.6.2",
    "buffer": "^6.0.3",
    "fast-deep-equal": "^3.1.3",
    "firebase": "^12.2.1",
    "outliers": "0.0.3",
    "rxjs": "^7.8.2",
    "semver": "^7.3.8"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^28.0.6",
    "@rollup/plugin-html": "^2.0.0",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^12.1.4",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.10.6",
    "@types/web-bluetooth": "^0.0.21",
    "@typescript-eslint/eslint-plugin": "^8.42.0",
    "@typescript-eslint/parser": "^8.42.0",
    "copyfiles": "^2.4.1",
    "dotenv": "^17.2.2",
    "eslint": "^9.17.0",
    "inquirer": "^12.9.4",
    "jest": "^29.7.0",
    "lite-server": "^2.6.1",
    "rollup": "^4.50.1",
    "rollup-plugin-dts": "^6.2.3",
    "ts-jest": "^29.2.5",
    "tslib": "^2.8.1",
    "typedoc": "^0.28.12",
    "typedoc-plugin-internal-external": "^2.2.0",
    "typedoc-plugin-markdown": "^4.8.1",
    "typescript": "^5.9.2"
  }
}
